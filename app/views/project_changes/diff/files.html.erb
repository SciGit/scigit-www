<% fileNum = 0
   @fileTypes.each do |type, type_data|
     unless @diff[type].empty? %>
       <li class="nav-header"><%= type_data[:name] %> Files</li>
       <% @diff[type].each do |file| %>
         <li><a class="hash-anchor" href="#change-file-<%= fileNum += 1 %>"><%= file.name %></a></li>
       <% end %>
  <% end %>
<% end %>

<% if fileNum == 0 %>
  <li class="nav-header">No files changed</li>
<% end %>
